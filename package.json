{
  "name": "@triniti/cms",
  "version": "1.0.0-beta.12",
  "license": "Apache-2.0",
  "description": "A single page app for managing triniti schemas and services.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/triniti/cms-js.git"
  },
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run build:babel && npm run build:prepub",
    "build:babel": "BABEL_ENV=build babel src --out-dir ./dist",
    "build:prepub": "cp -rf ./{*.md,package.json} ./dist && cpx 'src/**/*.{jpg,png,scss,sass,svg}' ./dist",
    "clean": "rm -rf ./dist",
    "pub": "npm run build && cd dist && npm publish --access public",
    "test": "NODE_NO_WARNINGS=1 NODE_OPTIONS=\"--experimental-loader @node-loader/babel -r ./bootstrap.cjs\" tape-es './src/**/*.test.js*' | faucet"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@gdbots/pbj": "^3.0.2",
    "@gdbots/pbjx": "^3.0.0",
    "@lexical/html": "^0.17.0",
    "@lexical/link": "^0.17.0",
    "@lexical/list": "^0.17.0",
    "@lexical/react": "^0.17.0",
    "@lexical/selection": "^0.17.0",
    "@lexical/utils": "^0.17.0",
    "bootstrap": "^5.3.3",
    "classnames": "^2.5.1",
    "core-js": "^3.37.1",
    "date-fns": "^3.6.0",
    "fast-deep-equal": "^3.1.3",
    "final-form": "^4.20.10",
    "final-form-arrays": "^3.1.0",
    "jwt-decode": "^4.0.0",
    "lexical": "^0.17.0",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "mime-types": "^2.1.35",
    "mqtt": "^5.6.1",
    "react": "^18.3.1",
    "react-datepicker": "^7.0.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-redux": "^9.1.2",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "react-select": "^5.8.0",
    "react-select-async-paginate": "^0.7.4",
    "reactstrap": "^9.2.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
