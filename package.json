  "scripts": {
    "build": "npm run clean && npm run lint && npm run test && npm run build:babel && npm run build:prepub",
    "build:babel": "BABEL_ENV=build babel src --out-dir ./dist",
    "build:prepub": "cp -rf ./{*.md,package.json} ./dist && cpx 'src/**/*.{scss,sass,svg}' ./dist",
    "clean": "rimraf ./dist",
    "init-git-hooks": "git init; find .git/hooks -type l -exec rm {} \\; && find .githooks -type f -exec ln -sf ../../{} .git/hooks/ \\;",
    "lint": "BABEL_ENV=eslint eslint src",
    "pub": "npm run build && cd dist && npm publish --access public",
    "test": "BABEL_ENV=test tape -r ./bootstrap.test.js './src/**/*.test.js*' | faucet"
  },
