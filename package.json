  "scripts": {
    "build": "npm run clean && npm run build:babel && npm run build:prepub",
    "build:babel": "BABEL_ENV=build babel src --out-dir ./dist",
    "build:prepub": "cp -rf ./{*.md,package.json} ./dist && cpx 'src/**/*.{scss,sass,svg,jpg,png}' ./dist",
    "clean": "rm -rf ./dist",
    "pub": "npm run build && cd dist && npm publish --access public",
    "test": "BABEL_ENV=test tape -r ./bootstrap.test.js './src/**/*.test.js*' | faucet",
    "build-vite": "npm run clean && vite build"
  },
  "workspaces": [
    "demo"
  ],
